 = db().select(db.zuoye.id).first().id\n    qtid = db.zuoye[firstzuoye].qingtong\n    byid = db.zuoye[firstzuoye].baiying\n    hjid = db.zuoye[firstzuoye].huangjing\n    qttm = db.timu[qtid]\n    bytm = db.timu[byid]\n    hjtm = db.timu[hjid]\n    a1 = A(\'\xe9\x9d\x92\xe9\x93\x9c\xef\xbc\x9a\' + qttm.biaoti, _href=URL(\'viewtimu\', args=[qtid]))\n    a2 = A(\'\xe7\x99\xbd\xe9\x93\xb6\xef\xbc\x9a\' + bytm.biaoti, _href=URL(\'viewtimu\', args=[byid]))\n    a3 = A(\'\xe9\xbb\x84\xe9\x87\x91\xef\xbc\x9a\' + hjtm.biaoti, _href=URL(\'viewtimu\', args=[hjid]))\n\n    return dict(left=left, a1=a1, a2=a2, a3=a3)\n\n\n@auth.requires_login()\ndef createtimu():\n    db.timu.zuozhe.default = auth.user.id\n    return dict(form=crud.create(db.timu, next=URL(\'timus\'), message=\'add sucessed\'))\n\n\n@auth.requires_login()\ndef edittimu():\n    return dict(form=crud.update(db.timu, request.args[0]))\n\n\n@auth.requires_login()\ndef tijiao():\n    timu = request.args[0]\n    if db.tijiao((db.tijiao.timu == timu) & (db.tijiao.zuozhe == auth.user.id)):\n        return dict(message=\'\xe5\xb7\xb2\xe7\xbb\x8f\xe6\x8f\x90\xe4\xba\xa4\xe8\xaf\xa5\xe9\xa2\x98\')\n    db.tijiao.timu.default = timu\n    db.tijiao.zuozhe.default = auth.user.id\n    form = crud.create(db.tijiao, netxt=URL(\'timus\'), message=\'\xe5\xb7\xb2\xe6\x8f\x90\xe4\xba\xa4\')\n    return locals()\n\n\n@auth.requires_login()\ndef createjietibaogao():\n    timu = request.args[0]\n    if db.jietibaogao((db.jietibaogao.timu == timu) & (db.jietibaogao.zuozhe == auth.user.id)):\n        return dict(message=\'\xe5\xb7\xb2\xe7\xbb\x8f\xe6\x8f\x90\xe4\xba\xa4\xe8\xaf\xa5\xe9\xa2\x98\xe8\xa7\xa3\xe9\xa2\x98\xe6\x8a\xa5\xe5\x91\x8a\')\n    db.jietibaogao.timu.default = timu\n    db.jietibaogao.zuozhe.default = auth.user.id\n    form = crud.create(db.jietibaogao, next=URL(\'jietibaogaos\',args=[timu]), message=\'\xe5\xb7\xb2\xe6\x8f\x90\xe4\xba\xa4\')\n    return locals()\n\n@auth.requires_login()\ndef tijiaos():\n    grid = SQLF